    </filter-mapping>
    <filter-mapping>
        <filter-name>logFilter</filter-name>
        <servlet-name>MessageHandler</servlet-name>
        <dispatcher>REQUEST</dispatcher>
        <dispatcher>FORWARD</dispatcher>
        <dispatcher>INCLUDE</dispatcher>
    </filter-mapping>
